{
  "name": "@caravan/buddy-reading-web-api",
  "version": "0.0.1-alpha.0",
  "description": "Web API for Caravan's buddy reading website",
  "main": "dist/index.js",
  "scripts": {
    "build": "npm run build:clean && tsc",
    "build:clean": "rimraf ./dist",
    "start": "cross-env NODE_ENV=production node ./dist/index.js",
    "dev": "cross-env NODE_ENV=development nodemon src/index.ts",
    "dev:watch": "cross-env NODE_ENV=development nodemon --inspect src/index.ts",
    "lint": "tsc --noEmit && eslint 'src/**/*.{js,ts,tsx}' --quiet --fix",
    "test": "jest"
  },
  "repository": "https://github.com/quinnturner/buddy-reading-website/tree/master/packages/web-api",
  "keywords": [
    "caravan",
    "buddy",
    "reading",
    "api"
  ],
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/quinnturner/buddy-reading-website/issues"
  },
  "homepage": "https://github.com/quinnturner/buddy-reading-website#README",
  "dependencies": {
    "axios": "^0.19.0",
    "body-parser": "^1.19.0",
    "btoa": "^1.2.1",
    "cookie-parser": "^1.4.4",
    "cookie-session": "^1.3.3",
    "cors": "^2.8.5",
    "cross-env": "^5.2.0",
    "discord.js": "^11.5.1",
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "express-validator": "^6.0.1",
    "helmet": "^3.18.0",
    "mongoose": "^5.6.1",
    "node-fetch": "^2.6.0",
    "slugify": "^1.3.4"
  },
  "devDependencies": {
    "@caravan/buddy-reading-types": "0.0.1-alpha.0",
    "@types/btoa": "^1.2.3",
    "@types/cookie-parser": "^1.4.1",
    "@types/cookie-session": "^2.0.37",
    "@types/cors": "^2.8.5",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.17.0",
    "@types/helmet": "^0.0.43",
    "@types/jest": "^24.0.15",
    "@types/mongoose": "^5.5.6",
    "@types/node": "^12.0.5",
    "@types/node-fetch": "^2.3.6",
    "@typescript-eslint/eslint-plugin": "^1.11.0",
    "@typescript-eslint/parser": "^1.11.0",
    "eslint": "^6.0.1",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-prettier": "^3.1.0",
    "jest": "^24.8.0",
    "nodemon": "^1.19.1",
    "prettier": "^1.18.2",
    "rimraf": "^2.6.3",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.3.0",
    "typescript": "^3.5.1",
    "utility-types": "^3.7.0"
  }
