{
  "name": "@caravanapp/web-api",
  "version": "1.0.0",
  "description": "Web API for Caravan's buddy reading website",
  "main": "dist/index.js",
  "scripts": {
    "build": "npm run build:clean && tsc",
    "build:clean": "rimraf ./dist",
    "build:mongo": "cd ../mongo && npm run build",
    "start": "cross-env NODE_ENV=production node ./dist/index.js",
    "dev": "cross-env NODE_ENV=development npm run build:mongo && nodemon src/index.ts",
    "dev:prebuilt": "cross-env NODE_ENV=development nodemon --inspect src/index.ts",
    "dev:watch": "cross-env NODE_ENV=development npm run build:mongo && nodemon --inspect src/index.ts",
    "lint": "eslint \"src/**/*\"",
    "lint:fix": "eslint \"src/**/*\" --quiet --fix",
    "test": "jest"
  },
  "repository": "https://github.com/quinnturner/buddy-reading-website/tree/master/packages/web-api",
  "keywords": [
    "caravan",
    "buddy",
    "reading",
    "api"
  ],
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/quinnturner/buddy-reading-website/issues"
  },
  "homepage": "https://github.com/quinnturner/buddy-reading-website#README",
  "dependencies": {
    "@caravanapp/mongo": "1.0.0",
    "@google-cloud/pubsub": "^0.30.3",
    "axios": "^0.19.2",
    "body-parser": "^1.19.0",
    "btoa": "^1.2.1",
    "cookie-parser": "^1.4.4",
    "cookie-session": "^1.4.0",
    "core-js": "^3.6.4",
    "cross-env": "^7.0.1",
    "date-fns": "^2.10.0",
    "discord.js": "^11.5.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-validator": "^6.4.0",
    "fuse.js": "^3.4.5",
    "helmet": "^3.21.3",
    "mongoose": "^5.6.8",
    "node-fetch": "^2.6.0",
    "slugify": "^1.4.0",
    "uuid": "^7.0.1"
  },
  "devDependencies": {
    "@caravanapp/types": "1.0.0",
    "@types/btoa": "^1.2.3",
    "@types/cookie-parser": "^1.4.2",
    "@types/cookie-session": "^2.0.38",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.3",
    "@types/helmet": "^0.0.45",
    "@types/jest": "^25.1.3",
    "@types/mongoose": "5.5.7",
    "@types/node": "^10.14.17",
    "@types/node-fetch": "^2.5.5",
    "@types/uuid": "^7.0.0",
    "@typescript-eslint/eslint-plugin": "^2.22.0",
    "@typescript-eslint/parser": "^2.22.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-jest": "^23.8.1",
    "eslint-plugin-prettier": "^3.1.2",
    "jest": "24.9.0",
    "nodemon": "^2.0.2",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.2",
    "ts-jest": "24.1.0",
    "ts-node": "^8.6.2",
    "typescript": "^3.8.3",
    "utility-types": "^3.10.0"
  }
}
